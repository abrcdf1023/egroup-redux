{
  "name": "@e-group/redux-modules",
  "version": "1.10.4",
  "private": false,
  "author": "eGroup Team",
  "description": "eGroup team react-redux modules that share across projects.",
  "keywords": [
    "eGroup",
    "react-redux",
    "redux-actions"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:abrcdf1023/egroup-redux.git",
    "directory": "packages/redux-modules"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abrcdf1023/egroup-redux/issues"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf build",
    "build": "yarn clean && yarn build:esm && yarn build:tsd && yarn build:copy-files",
    "build:tsd": "tsc ./src/index.ts --declaration --jsx react --esModuleInterop --emitDeclarationOnly && tsc ./src/immutable/index.ts --declaration --jsx react --esModuleInterop --emitDeclarationOnly",
    "build:esm": "babel --config-file ../../babel.config.js ./src --out-dir ./build --extensions .js,.ts,.tsx",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prepare": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rimraf": "^2.6.3"
  },
  "gitHead": "962c60a7aa37f395e04272ef4ad417cd8fb6af15",
  "peerDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux-actions": "^2.6.1",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux-actions": "^2.6.5"
  },
  "dependencies": {
    "@e-group/utils": "^6.23.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/warning": "^3.0.0",
    "immer": "^7.0.1",
    "lodash": "^4.17.15",
    "lodash.assign": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "warning": "^4.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
